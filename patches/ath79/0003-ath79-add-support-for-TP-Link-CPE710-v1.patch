 .../generic/base-files/etc/board.d/01_leds    |   3 +-
 .../generic/base-files/etc/board.d/02_network |   1 +
 .../etc/hotplug.d/firmware/11-ath10k-caldata  |   6 +
 target/linux/ath79/image/generic-tp-link.mk   |  11 ++
